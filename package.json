{
  "name": "@ilya_burlak/ui-kit",
  "version": "0.0.1",
  "description": "A small React + TypeScript UI Kit with SCSS components",
  "type": "module",
  "scripts": {
    "dev": "webpack serve --config webpack.config.cjs --mode development",
  "build:css": "sass src/styles/index.scss dist/ui-kit.css --no-source-map --style=expanded",
  "build": "webpack --config webpack.config.cjs --mode production && npm run build:css && tsc -p tsconfig.build.json",
  "lint": "eslint \"**/*.{js,jsx}\" --fix --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss,json,md}\"",
    "type-check": "tsc --noEmit",
  "check": "tsc --noEmit && eslint \"**/*.{js,jsx}\" --report-unused-disable-directives --max-warnings 0",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "npm run build"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/react-webpack5": "^9.1.13",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "css-loader": "^6.8.1",
    "eslint": "^9.36.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^5.2.0",
  "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-storybook": "^9.1.13",
    "globals": "^16.4.0",
    "mini-css-extract-plugin": "^2.8.2",
    "prettier": "^3.0.0",
    "sass": "^1.72.0",
    "sass-loader": "^13.3.1",
    "storybook": "^9.1.13",
    "ts-loader": "^9.4.2",
    "typescript": "~5.9.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.0"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/ui-kit.css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaBurlak/Kodex-UI-KIT.git"
  },
  "keywords": ["ui","react","typescript","scss","components"],
  "author": "Ilya Burlak <ilya.burlak.dev@mail.ru>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "style": "dist/ui-kit.css",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  }
}